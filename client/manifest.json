{
	"name": "Local proctoring",
	"description": "Local screencast",
	"version": "0.1",
	"manifest_version": 3,
	"permissions": [
		"storage",
		"alarms"
	],
	"action": {
		"default_popup": "index.html"
	},
	"background": {
    	"service_worker": "background.js",
		"type": "module"
  	},
	"web_accessible_resources": [
        {
        "resources": ["media.html", "media.js"],
        "matches": ["<all_urls>"]
        }
    ]
}
